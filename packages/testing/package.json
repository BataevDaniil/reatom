    "test": "nx test:native @reatom/testing",
    "test:native": "cross-env fixme=fixme tsx src/index.test.ts"
  },
  "author": "artalar",
  "license": "MIT",
  "files": [
    "/build",
    "/package.json",
    "/README.md",
    "/src"
