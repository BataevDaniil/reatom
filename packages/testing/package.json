{
  "name": "@reatom/testing",
  "version": "3.0.0",
  "publishConfig": {
    "tag": "alpha"
  },
  "private": false,
  "sideEffects": false,
  "description": "Reatom internal utils",
  "source": "src/index.ts",
  "exports": {
    "require": "./build/index.js",
    "default": "./build/index.module.js"
  },
  "main": "build/index.js",
  "module": "build/index.module.js",
  "unpkg": "build/index.umd.js",
